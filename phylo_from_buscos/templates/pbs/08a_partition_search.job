#!/bin/bash
#PBS -N iqtree_partition
#PBS -l nodes=1:ppn=18
#PBS -l mem=72gb
#PBS -l walltime=72:00:00

cd $PBS_O_WORKDIR/06_concatenation
source ~/.bashrc
conda activate phylo

iqtree \
  -s FcC_supermatrix.fas \
  -spp partition_def.txt \
  -nt 18 \
  -pre partition_search \
  -m TESTMERGEONLY \
  -mset MODEL_SET \
  -msub nuclear \
  -rcluster 10 \
  -bb 1000 \
  -alrt 1000
